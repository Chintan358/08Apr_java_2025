package oops;

class P
{
	P()
	{
		System.out.println("P class const. calling...");
	}
}

class Q
{
	Q()
	{
		System.out.println("Q class const. calling...");
	}
}

class R extends Q
{
		R()
		{
			System.out.println("R class const. calling...");
		}
}

public class O006_constructorChainnig {
	public static void main(String[] args) {
		
		R r  = new R();
		
		
	}
}
