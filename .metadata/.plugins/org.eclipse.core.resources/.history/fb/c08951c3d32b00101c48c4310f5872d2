package oops;

class Pen
{
	//data member
	int price = 10;
	String color = "Red";
	String company = "Cello";
	
	//function memebr
	public void toWrite()
	{
		System.out.println(price+" "+color+" "+company);
	}
	
}

public class O001_ClassDemo {
	public static void main(String[] args) {
		
		Pen p1 = new Pen();
		
		p1.toWrite();
		
	}
}
