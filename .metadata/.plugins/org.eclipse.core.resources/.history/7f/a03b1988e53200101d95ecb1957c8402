package oops;

public class Emp {
	
		private int id;
		private String name;
		private String email;
		
		public void setdata(int id, String name, String email)
		{
			this.id  =id;
			this.name = name;
			this.email = email;
			
		}
		
		public void display()
		{
			System.out.println(id+" "+name+" "+email);
		}
}
