package logical;

public class Armstrong {
	public static void main(String[] args) {
		
		//153 1+125+27 = 153
		//1634 = 1 + 1296 + 81 + 256
		
		int number = 153;
		
		while(number !=0)
		{
			int rem = number%10;
			System.out.println(rem);
			number = number/10;
			System.out.println(number);
		}
		
		
	}
}
