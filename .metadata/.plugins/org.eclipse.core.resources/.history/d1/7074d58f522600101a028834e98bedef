package logical;

public class Pettern4 {
	public static void main(String[] args) {
		
//		    *
//		   **
//		  ***
//		 ****
//		*****
		
		for(int j=5;j>=1;j--)
		{
			for(int k=1;k<j;k++)
			{
				System.out.print(" ");
			}
			
			for(int i=1;i<=1;i++)
			{
				System.out.print("*");
			}
			System.out.println();
		}	
		
	
	}
}
