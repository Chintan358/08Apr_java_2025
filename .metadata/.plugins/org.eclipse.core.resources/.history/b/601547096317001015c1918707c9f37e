package basic;

public class B002_Datatpes {
	public static void main(String[] args) {
		
		
		byte b = 127;
		short s  =32767;
		int i = 4545466;
		long l = 56456456456456l;
		
		System.out.println(l);
		
		float f = 45.564564564f;
		double d = 456.65466465646;
		
		System.out.println(f);
		System.out.println(d);
		
		char ch  ='A';
		System.out.println(ch);
		
		boolean bool = true;
		System.out.println(bool);
		
	}
}	
